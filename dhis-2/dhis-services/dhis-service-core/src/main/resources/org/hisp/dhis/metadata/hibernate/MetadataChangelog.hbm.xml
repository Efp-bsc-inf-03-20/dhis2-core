<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
  [<!ENTITY identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm">]
  >

<hibernate-mapping>
  <class name="org.hisp.dhis.metadata.changelog.MetadataChangelog" table="metadatachangelog">

    <id name="id" column="metadatachangelogid">
      <generator class="native" />
    </id>

    <property name="name" column="name" not-null="true" unique="true" length="250" />

    <property name="type" column="type" length="50"/>

    <property name="version" column="version" not-null="true" length="10" />

    <property name="dhis2Version" column="dhisversion" not-null="true" length="10" />

    <property name="locale" column="locale" not-null="true" length="10" />

    <property name="created" type="timestamp" not-null="true" />

    <many-to-one name="importFile" class="org.hisp.dhis.fileresource.FileResource" column="importfile" foreign-key="fk_metadatachangelog_fileresourceid" />

    <many-to-one name="createdBy" class="org.hisp.dhis.user.User" column="createdby" foreign-key="fk_metadatachangelog_userid" />

  </class>
</hibernate-mapping>
